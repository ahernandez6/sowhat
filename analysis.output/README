#Commands used in the analysis of each data set:

#protein.benchmark
#ANALYSIS 1
perl sowh.pl --constraint=published_datasets/protein.t2.tre --aln=published_datasets/protein.benchmark.phy --model=PROTGAMMAWAG --dir=test.output/9.protein.t2 --name=9.protein.t2 --tests=100 >output.9.prot.sowh.t2

#ANALYSIS 2
perl sowh.pl --constraint=published_datasets/protein.t1.tre --aln=published_datasets/protein.benchmark.phy --model=PROTGAMMAWAG --dir=test.output/1.protein.t1 --name=1.protein.t1 --tests=100 >output.1.prot.sowh.t1

#ANALYSIS 3
perl sowh.pl --constraint=published_datasets/protein.t2.tre --aln=published_datasets/protein.benchmark.phy --model=PROTGAMMAWAG --dir=test.output/9.protein.t2 --name=9.protein.t2 --tests=100 >output.9.prot.sowh.t2

#ANALYSIS 4
perl sowh.pl --constraint=published_datasets/protein.t2.tre --aln=published_datasets/protein.benchmark.phy --model=PROTGAMMAWAG --dir=test.output/10.protein.t2 --name=10.protein.t2 --reps=1000  --sowhat >output.10.prot.sowhat.t2

