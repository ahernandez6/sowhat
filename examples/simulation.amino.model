partition = 0
datatype = aminoacid
model = GTR
rate file = examples/simulation.rates
frequencies = 0.087 0.044 0.039 0.057 0.019 0.037 0.058 0.083 0.024 0.048 0.086 0.062 0.020 0.038 0.046 0.070 0.061 0.014 0.035 0.071
alpha = 0.7527
